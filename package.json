{
  "name": "ms-payments",
  "description": "Core of the microservice for handling payments",
  "version": "0.0.0-development",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "rimraf ./lib && babel -d ./lib --copy-files ./src",
    "pretest": "rimraf ./ss/*; mkdirp ./ss",
    "test": "yarn lint && yarn test:e2e",
    "test:e2e": "mdep test run",
    "lint": "eslint ./src",
    "start": "mfleet",
    "prepublishOnly": "yarn compile",
    "semantic-release": "semantic-release",
    "docker-release": "mdep docker release",
    "release": "yarn --frozen-lockfile && yarn semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/ms-payments.git"
  },
  "author": "Dmitry Gorbunov <me@tewi.tk>",
  "contributors": [
    "Vitaly Aminev <v@makeomatic.ca>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/makeomatic/ms-payments/issues"
  },
  "homepage": "https://github.com/makeomatic/ms-payments#readme",
  "dependencies": {
    "@microfleet/core": "^13.3.1",
    "@microfleet/transport-amqp": "^13.1.2",
    "@microfleet/validation": "^8.1.0",
    "@sentry/node": "^4.2.3",
    "bluebird": "^3.5.2",
    "bunyan": "^1.8.12",
    "bunyan-sentry-stream": "^1.1.0",
    "bytes": "^3.0.0",
    "cluster-key-slot": "^1.0.12",
    "common-errors": "^1.0.5",
    "debug": "^4.1.0",
    "ioredis": "^4.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "ms-conf": "^3.3.1",
    "ms-mailer-client": "^8.0.1",
    "ms-mailer-templates": "^1.10.0",
    "paypal-rest-sdk": "^1.8.1",
    "redis-filtered-sort": "^2.3.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.1",
    "stdout-stream": "^1.4.1",
    "urlsafe-base64": "^1.0.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@makeomatic/deploy": "^8.2.3",
    "@semantic-release/exec": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "bluebird-retry": "^0.11.0",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-makeomatic": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-promise": "^4.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "puppeteer": "=1.4.0",
    "rimraf": "^2.6.1",
    "sinon": "^7.1.0"
  },
  "engine": {
    "node": ">= 8.9.0"
  },
  "files": [
    "bin/",
    "lib/",
    "src/",
    "schemas/",
    "yarn.lock"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $HUSKY_GIT_PARAMS"
    }
  }
}
